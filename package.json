    "@nimbl3/eslint-config-nimbl3": "2.1.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "eslint": "5.6.0",
    "eslint-plugin-vue": "4.7.1",
    "svg-spritemap-webpack-plugin": "2.7.0",
